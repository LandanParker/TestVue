@page
@{
    ViewData["Title"] = "Home page";
}

<head>
    <script src="https://cdn.jsdelivr.net/npm/vue@2/dist/vue.js"></script>
</head>

<div id="app">
  {{ message }}
</div>

<script>
    var app = new Vue({
    el: '#app',
    data: {
    message: 'Hello Vue!'
    }
    })
    
    setTimeout(()=>{
        app.message = "changed";
    }, 2000)
</script>
